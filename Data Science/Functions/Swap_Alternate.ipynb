   "source": [
    "def swap_consecutive(l):\n",
    "    i = 0\n",
    "    while i + 1 < len(l):\n",
    "        l[i], l[i + 1] = l[i + 1], l[i]\n",
    "        i = i + 2"
   ]
  },
